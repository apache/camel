        <xs:enumeration value="new-product"/>
        <xs:enumeration value="status-update"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="time-granularity">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="day"/>
        <xs:enumeration value="month"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

</xs:schema>
