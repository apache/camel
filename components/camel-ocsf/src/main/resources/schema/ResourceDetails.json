{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schema.ocsf.io/1.7.0/objects/resource_details",
  "title": "ResourceDetails",
  "type": "object",
  "javaType": "org.apache.camel.dataformat.ocsf.model.ResourceDetails",
  "additionalProperties": true,
  "properties": {
    "data": {
      "type": [
        "object",
        "string",
        "integer",
        "number",
        "boolean",
        "array",
        "null"
      ],
      "title": "Data"
    },
    "name": {
      "type": "string",
      "title": "Name"
    },
    "owner": {
      "$ref": "User.json"
    },
    "type": {
      "type": "string",
      "title": "Type"
    },
    "version": {
      "type": "string",
      "title": "Version"
    },
    "ip": {
      "type": "string",
      "title": "IP Address",
      "pattern": "((^\\s*((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]).){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]))\\s*$)|(^\\s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:)))(%.+)?\\s*$))"
    },
    "group": {
      "$ref": "Group.json"
    },
    "hostname": {
      "type": "string",
      "title": "Hostname"
    },
    "uid": {
      "type": "string",
      "title": "Unique ID"
    },
    "labels": {
      "type": "array",
      "title": "Labels",
      "items": {
        "type": "string"
      }
    },
    "namespace": {
      "type": "string",
      "title": "Namespace"
    },
    "role": {
      "type": "string",
      "title": "Role"
    },
    "tags": {
      "type": "array",
      "title": "Tags",
      "items": {
        "type": "object",
        "additionalProperties": true
      }
    },
    "agent_list": {
      "type": "array",
      "description": "",
      "items": {
        "$ref": "Agent.json"
      }
    },
    "created_time": {
      "type": "integer",
      "title": "Created Time",
      "existingJavaType": "java.lang.Long"
    },
    "criticality": {
      "type": "string",
      "title": "Criticality"
    },
    "is_backed_up": {
      "type": "boolean",
      "title": "Back Ups Configured"
    },
    "modified_time": {
      "type": "integer",
      "title": "Modified Time",
      "existingJavaType": "java.lang.Long"
    },
    "resource_relationship": {
      "type": "object",
      "additionalProperties": true
    },
    "role_id": {
      "type": "integer",
      "title": "Role ID"
    },
    "uid_alt": {
      "type": "string",
      "title": "Alternate ID"
    }
  },
  "description": "The Resource Details object."
}