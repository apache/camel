{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schema.ocsf.io/1.7.0/objects/device",
  "title": "Device",
  "type": "object",
  "javaType": "org.apache.camel.dataformat.ocsf.model.Device",
  "additionalProperties": true,
  "properties": {
    "uid": {
      "type": "string",
      "title": "Unique ID"
    },
    "desc": {
      "type": "string",
      "title": "Description"
    },
    "is_backed_up": {
      "type": "boolean",
      "title": "Back Ups Configured"
    },
    "is_managed": {
      "type": "boolean",
      "title": "Managed Device"
    },
    "location": {
      "$ref": "Location.json"
    },
    "org": {
      "$ref": "Organization.json"
    },
    "risk_score": {
      "type": "integer",
      "title": "Risk Score"
    },
    "interface_name": {
      "type": "string",
      "title": "Network Interface Name"
    },
    "hypervisor": {
      "type": "string",
      "title": "Hypervisor"
    },
    "eid": {
      "type": "string",
      "title": "EID"
    },
    "image": {
      "$ref": "Image.json"
    },
    "zone": {
      "type": "string",
      "title": "Network Zone"
    },
    "subnet_uid": {
      "type": "string",
      "title": "Subnet UID"
    },
    "iccid": {
      "type": "string",
      "title": "ICCID"
    },
    "mac": {
      "type": "string",
      "title": "MAC Address",
      "pattern": "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$"
    },
    "created_time": {
      "type": "integer",
      "title": "Created Time",
      "existingJavaType": "java.lang.Long"
    },
    "network_interfaces": {
      "type": "array",
      "description": "",
      "items": {
        "$ref": "NetworkInterface.json"
      }
    },
    "model": {
      "type": "string",
      "title": "Model"
    },
    "owner": {
      "$ref": "User.json"
    },
    "os_machine_uuid": {
      "type": "string",
      "title": "OS Machine UUID",
      "pattern": "[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}"
    },
    "os": {
      "$ref": "Os.json"
    },
    "vlan_uid": {
      "type": "string",
      "title": "VLAN"
    },
    "meid": {
      "type": "string",
      "title": "MEID"
    },
    "vpc_uid": {
      "type": "string",
      "title": "VPC UID"
    },
    "boot_time": {
      "type": "integer",
      "title": "Boot Time",
      "existingJavaType": "java.lang.Long"
    },
    "is_compliant": {
      "type": "boolean",
      "title": "Compliant Device"
    },
    "is_personal": {
      "type": "boolean",
      "title": "Personal Device"
    },
    "ip": {
      "type": "string",
      "title": "IP Address",
      "pattern": "((^\\s*((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]).){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]))\\s*$)|(^\\s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:)))(%.+)?\\s*$))"
    },
    "is_supervised": {
      "type": "boolean",
      "title": "Supervised Device"
    },
    "first_seen_time": {
      "type": "integer",
      "title": "First Seen",
      "existingJavaType": "java.lang.Long"
    },
    "uid_alt": {
      "type": "string",
      "title": "Alternate ID"
    },
    "type_id": {
      "type": "integer",
      "title": "Type ID"
    },
    "imei": {
      "type": "string",
      "title": "IMEI"
    },
    "autoscale_uid": {
      "type": "string",
      "title": "Autoscale UID"
    },
    "groups": {
      "type": "array",
      "title": "Groups",
      "items": {
        "$ref": "Group.json"
      }
    },
    "agent_list": {
      "type": "array",
      "description": "",
      "items": {
        "$ref": "Agent.json"
      }
    },
    "hw_info": {
      "type": "object",
      "additionalProperties": true
    },
    "domain": {
      "type": "string",
      "title": "Domain"
    },
    "instance_uid": {
      "type": "string",
      "title": "Instance ID"
    },
    "subnet": {
      "type": "string",
      "title": "Subnet"
    },
    "is_mobile_account_active": {
      "type": "boolean",
      "title": "Mobile Account Active"
    },
    "risk_level_id": {
      "type": "integer",
      "title": "Risk Level ID"
    },
    "modified_time": {
      "type": "integer",
      "title": "Modified Time",
      "existingJavaType": "java.lang.Long"
    },
    "last_seen_time": {
      "type": "integer",
      "title": "Last Seen",
      "existingJavaType": "java.lang.Long"
    },
    "udid": {
      "type": "string",
      "title": "Unique Device Identifier"
    },
    "region": {
      "type": "string",
      "title": "Region"
    },
    "name": {
      "type": "string",
      "title": "Name"
    },
    "boot_uid": {
      "type": "string",
      "title": "Boot UID"
    },
    "is_shared": {
      "type": "boolean",
      "title": "Shared Device"
    },
    "is_trusted": {
      "type": "boolean",
      "title": "Trusted Device"
    },
    "interface_uid": {
      "type": "string",
      "title": "Network Interface ID"
    },
    "type": {
      "type": "string",
      "title": "Type"
    },
    "imei_list": {
      "type": "array",
      "title": "IMEI List",
      "items": {
        "type": "string"
      }
    },
    "vendor_name": {
      "type": "string",
      "title": "Vendor Name"
    },
    "risk_level": {
      "type": "string",
      "title": "Risk Level"
    },
    "hostname": {
      "type": "string",
      "title": "Hostname"
    }
  },
  "description": "The Device object."
}