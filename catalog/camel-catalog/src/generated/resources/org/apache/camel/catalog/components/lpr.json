    "sendToPrinter": { "kind": "parameter", "displayName": "Send To Printer", "group": "producer", "label": "", "required": false, "type": "boolean", "javaType": "boolean", "deprecated": false, "secret": false, "defaultValue": true, "configurationClass": "org.apache.camel.component.printer.PrinterConfiguration", "configurationField": "config", "description": "etting this option to false prevents sending of the print data to the printer" },
